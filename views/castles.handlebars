<div class='main'>
  <h1 class='highlighted'>Castles of {{castles.kingdom}} Kingdom</h1>
  <div class="container">
    {{#each castles as |castle|}}
    <div class="kingdom focus" id={{castle.id}}>
      <div class='image'>
        <a href={{castle}}>
          <h2 class="grayed">{{castle.name}}</h2>
        </a>
      </div>
      <section>
        <h4 class="grayed"># of Lieges: {{castle.liegeIds.length}}</h4>
      </section>
    </div>
    {{/each}}
  </div>
