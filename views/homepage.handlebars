<div class='main'>
  <h1 class='highlighted'>Kingdoms</h1>
  <div class="container">
    {{#each kingdoms as |kingdom|}}
    <div class="kingdom focus" id={{kingdom.id}}>
      <div class='image'>
        <a href={{kingdom}}>
          <h2 class="grayed">{{kingdom.name}}</h2>
        </a>
      </div>
      <section>
        <h3 class="grayed">{{kingdom.king}} King</h3>
        <h3 class="grayed">{{kingdom.queen}} Queen</h3>
      </section>
    </div>
    {{/each}}
  </div>
